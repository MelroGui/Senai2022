package excartao;

public class Cartao {

	public static void main(String[] args) {
		System.out.println("O algoritimo de acumulação");
		System.out.print("Simples, retornou = ");
		System.out.println(simples());

	}
	
	public static int simples() {
		int x = 0;
		for(int i = 0; i < 10; i++);
			x++;
			return x;
	}

}
